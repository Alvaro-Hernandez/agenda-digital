{
  "expo": {
    "name": "EduPlanner",
    "slug": "EduPlanner",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/libro.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/libro.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.carlos.eduplanner",
      "adaptiveIcon": {
        "foregroundImage": "./assets/libro.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/libro.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "94e1298d-64f7-4e2b-90ef-43fe0923b038"
      }
    }
  }
}
